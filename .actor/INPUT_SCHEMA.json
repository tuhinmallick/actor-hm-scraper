        "filters": {
            "title": "Product Filters",
            "type": "object",
            "description": "Filter products by various criteria",
            "editor": "json",
            "properties": {
                "minPrice": {
                    "title": "Min Price",
                    "type": "integer",
                    "description": "Minimum price in local currency",
                    "minimum": 0
                },
                "maxPrice": {
                    "title": "Max Price",
                    "type": "integer",
                    "description": "Maximum price in local currency",
                    "minimum": 0
                },
                "colors": {
                    "title": "Colors",
                    "type": "array",
                    "description": "Filter by colors (e.g., 'black', 'white', 'red', 'blue')",
                    "editor": "stringList"
                },
                "sizes": {
                    "title": "Sizes",
                    "type": "array",
                    "description": "Filter by sizes (e.g., 'XS', 'S', 'M', 'L', 'XL')",
                    "editor": "stringList"
                },
                "materials": {
                    "title": "Materials",
                    "type": "array",
                    "description": "Filter by materials (e.g., 'cotton', 'polyester', 'wool')",
                    "editor": "stringList"
                },
                "patterns": {
                    "title": "Patterns",
                    "type": "array",
                    "description": "Filter by patterns (e.g., 'solid', 'striped', 'floral')",
                    "editor": "stringList"
                },
                "fits": {
                    "title": "Fits",
                    "type": "array",
                    "description": "Filter by fit (e.g., 'slim', 'regular', 'oversized')",
                    "editor": "stringList"
                },
                "lengths": {
                    "title": "Lengths",
                    "type": "array",
                    "description": "Filter by length (e.g., 'mini', 'midi', 'maxi')",
                    "editor": "stringList"
                },
                "sale": {
                    "title": "Sale Items Only",
                    "type": "boolean",
                    "description": "Only get products on sale"
                },
                "sustainable": {
                    "title": "Sustainable Only",
                    "type": "boolean",
                    "description": "Only get sustainable/conscious products"
                },
                "newArrivals": {
                    "title": "New Arrivals Only",
                    "type": "boolean",
                    "description": "Only get new arrival products"
                }
            }
        },
        "maxProducts": {
            "title": "Maximum Products",
            "type": "integer",
            "description": "Maximum number of products to scrape (0 = unlimited)",
            "default": 100,
            "minimum": 0,
            "maximum": 50000
        },
        "maxPages": {
            "title": "Maximum Pages",
            "type": "integer",
            "description": "Maximum pages to scrape per category (0 = unlimited)",
            "default": 0,
            "minimum": 0
        },
        "productsPerPage": {
            "title": "Products Per Page",
            "type": "integer",
            "description": "Number of products per page (higher = faster)",
            "default": 72,
            "minimum": 1,
            "maximum": 200
        },
        "sortBy": {
            "title": "Sort Products By",
            "type": "string",
            "description": "How to sort products",
            "default": "stock",
            "editor": "select",
            "enum": ["stock", "newProduct", "ascPrice", "descPrice"],
            "enumTitles": ["Relevance", "Newest First", "Price: Low to High", "Price: High to Low"]
        },
        "includeVariants": {
            "title": "Include All Variants",
            "type": "boolean",
            "description": "Extract all color and size variants",
            "default": true
        },
        "extractProductDetails": {
            "title": "Extract Full Details",
            "type": "boolean",
            "description": "Visit product pages for complete information (slower but more data)",
            "default": false
        },
        "includeStoreAvailability": {
            "title": "Include Store Availability",
            "type": "boolean",
            "description": "Check availability in physical stores",
            "default": false
        },
        "outputFields": {
            "title": "Output Fields",
            "type": "array",
            "description": "Select specific fields to include (empty = all fields)",
            "editor": "select",
            "default": [],
            "uniqueItems": true,
            "items": {
                "type": "string",
                "editor": "select",
                "enum": [
                    "productId", "title", "description", "price", "originalPrice",
                    "salePrice", "currency", "colors", "sizes", "materials",
                    "images", "videos", "category", "url", "inStock", "stockLevel",
                    "sustainable", "rating", "reviews"
                ],
                "enumTitles": [
                    "Product ID", "Title", "Description", "Price", "Original Price",
                    "Sale Price", "Currency", "Colors", "Sizes", "Materials",
                    "Images", "Videos", "Category", "URL", "In Stock", "Stock Level",
                    "Sustainable", "Rating", "Reviews"
                ]
            }
        },
        "includeRawData": {
            "title": "Include Raw Data",
            "type": "boolean",
            "description": "Include original H&M data structure",
            "default": false
        },
        "includeTranslations": {
            "title": "Include Original Language",
            "type": "boolean",
            "description": "Include product info in original market language",
            "default": false
        },
        "proxyConfiguration": {
            "title": "Proxy Configuration",
            "type": "object",
            "description": "Use proxies to avoid blocking",
            "prefill": { "useApifyProxy": true },
            "editor": "proxy"
        },
        "maxConcurrency": {
            "title": "Max Concurrency",
            "type": "integer",
            "description": "Maximum parallel requests",
            "default": 5,
            "minimum": 1,
            "maximum": 50
        },
        "requestTimeout": {
            "title": "Request Timeout",
            "type": "integer",
            "description": "Timeout per request in seconds",
            "default": 60,
            "minimum": 30,
            "maximum": 300
        },
        "enableAntiBot": {
            "title": "Anti-Bot Protection",
            "type": "boolean",
            "description": "Use advanced anti-detection (recommended)",
            "default": true
        },
        "enableSmartExtraction": {
            "title": "Smart Extraction",
            "type": "boolean",
            "description": "Use AI-powered extraction with fallbacks",
            "default": true
        },
        "enableProgressiveSaving": {
            "title": "Progressive Saving",
            "type": "boolean",
            "description": "Save data in batches during scraping",
            "default": true
        },
        "debug": {
            "title": "Debug Mode",
            "type": "boolean",
            "description": "Enable detailed logging",
            "default": false
        }
    },
    "required": []
}